{
  "name": "molecule-polarity",
  "version": "1.4.0-dev.3",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/molecule-polarity.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "MOLECULE_POLARITY",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "phetLibs": [
      "mobius",
      "nitroglycerin"
    ],
    "preload": [
      "../sherpa/lib/three-r160.js",
      "../sherpa/lib/three-subdivide-1.1.3.js"
    ],
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsInteractiveDescription": true,
      "supportsDynamicLocale": true,
      "colorProfiles": [
        "default"
      ]
    },
    "published": true,
    "screenNameKeys": [
      "MOLECULE_POLARITY/screen.twoAtoms",
      "MOLECULE_POLARITY/screen.threeAtoms",
      "MOLECULE_POLARITY/screen.realMolecules"
    ]
  }
}